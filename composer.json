{
    "name": "kazakevic/strapi-wrapper",
    "description": "wrapper for Strapi headless CMS",
    "type": "library",
    "keywords": ["strapi php", "strapi client"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kazakevic\\StrapiWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StrapiWrapper\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Darjus Kazakevic",
            "email": "darjus.kazakevic@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.1",
        "phpunit/phpunit": "^9"
    },
    "suggest": {
        "guzzlehttp/guzzle": "use it as HTTP client"
    },
    "config": {
        "sort-packages": true
    }
}
